var sum=0;
let marks=[91,92,63,84];
console.log(marks);
marks.pop();
marks.push(5);
console.log(marks);
for( var i=0;i<4;i++)
  {
    sum+=sum+marks[i]; 
  }
console.log(sum);
let b=marks.toString()
console.log(b);
let c=marks.join("_");
console.log(c,typeof c);
console.log(marks.shift());
for (i=0;i<=3;i++) 
  {
    console.log(marks[i]);
  }
  
marks.forEach((element)=>
  {
    console.log(element * element)
  })












